{
	"name": "aline",
	"displayName": "Aline - CLINE, Chatbot, DeepSeek and more",
	"description": "Forked version of Cline, introducing flexible and user-friendly features, such as: Customize Your Chatbot, Translator, and more. Autonomous coding agent right in your IDE, capable of creating/editing files, running commands, using the browser",
	"version": "3.1.10",
	"icon": "assets/icons/icon.png",
	"galleryBanner": {
		"color": "#617A91",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.84.0"
	},
	"author": {
		"name": "Dream Light"
	},
	"license": "Apache-2.0",
	"publisher": "DreamLight",
	"repository": { 
		"type": "git",
		"url": "https://github.com/debug-dream/aline.git"
	},
	"homepage": "",
	"categories": [
		"AI",
		"Chat",
		"Programming Languages",
		"Education",
		"Snippets",
		"Testing"
	],
	"keywords": [
		"cline",
		"claude",
		"dev",
		"mcp",
		"openrouter",
		"coding",
		"agent",
		"autonomous",
		"chatgpt",
		"deepseek",
		"ai",
		"llama"
	],
	"activationEvents": [],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "aline-dev-ActivityBar",
					"title": "Aline",
					"icon": "assets/icons/icon.png"
				}
			]
		},
		"views": {
			"aline-dev-ActivityBar": [
				{
					"type": "webview",
					"id": "aline-dev.SidebarProvider",
					"name": ""
				}
			]
		},
		"commands": [
			{
				"command": "aline.plusButtonClicked",
				"title": "New Task",
				"icon": "$(add)"
			},
			{
				"command": "aline.mcpButtonClicked",
				"title": "MCP Servers",
				"icon": "$(server)"
			},
			{
				"command": "aline.historyButtonClicked",
				"title": "History",
				"icon": "$(history)"
			},
			{
				"command": "aline.popoutButtonClicked",
				"title": "Open in Editor",
				"icon": "$(link-external)"
			},
			{
				"command": "aline.settingsButtonClicked",
				"title": "Settings",
				"icon": "$(settings-gear)"
			},
			{
				"command": "aline.openInNewTab",
				"title": "Open In New Tab",
				"category": "Aline"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "aline.plusButtonClicked",
					"group": "navigation@1",
					"when": "view == aline-dev.SidebarProvider"
				},
				{
					"command": "aline.mcpButtonClicked",
					"group": "navigation@2",
					"when": "view == aline-dev.SidebarProvider"
				},
				{
					"command": "aline.historyButtonClicked",
					"group": "navigation@3",
					"when": "view == aline-dev.SidebarProvider"
				},
				{
					"command": "aline.popoutButtonClicked",
					"group": "navigation@4",
					"when": "view == aline-dev.SidebarProvider"
				},
				{
					"command": "aline.settingsButtonClicked",
					"group": "navigation@5",
					"when": "view == aline-dev.SidebarProvider"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "npm run check-types && npm run lint && node esbuild.js",
		"build:chatview": "vite -c chatview-ui/vite.config.ts build",
		"build:parallel": "npm-run-all --parallel build:chatview build:webview",
		"watch": "npm-run-all --parallel watch:*",
		"watch:esbuild": "node esbuild.js --watch",
		"watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
		"package": "npm run build:parallel && npm run check-types && npm run lint && node esbuild.js --production",
		"compile-tests": "tsc -p ./tsconfig.test.json --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"check-types": "tsc --noEmit",
		"lint": "eslint src --ext ts",
		"format": "prettier . --check",
		"format:fix": "prettier . --write",
		"test": "vscode-test",
		"install:all": "npm install && cd webview-ui && npm install && cd ../lib/ui && npm install && cd ../shared && npm install",
		"start:webview": "cd webview-ui && npm run start",
		"build:webview": "cd webview-ui && npm run build",
		"test:webview": "cd webview-ui && npm run test",
		"install:dev": "vsce package --out build/out.vsix && code --install-extension build/out.vsix",
		"start:dev": "NODE_ENV=development code --extensionDevelopmentPath=$PWD --disable-extension=github.copilot --inspect-extensions=9333 --new-window . --goto ./src/extension.ts:16:5",
		"publish:marketplace": "vsce publish && ovsx publish",
		"watch:all": "npm-run-all --parallel watch:esbuild watch:tsc watch:chatview",
		"watch:chatview": "vite -c chatview-ui/vite.config.ts --watch"
	},
	"devDependencies": {
		"@types/diff": "^5.2.1",
		"@types/mocha": "^10.0.7",
		"@types/node": "20.x",
		"@types/should": "^11.2.0",
		"@types/vscode": "^1.84.0",
		"@typescript-eslint/eslint-plugin": "^7.14.1",
		"@typescript-eslint/parser": "^7.11.0",
		"@vscode/test-cli": "^0.0.9",
		"@vscode/test-electron": "^2.4.0",
		"esbuild": "^0.21.5",
		"eslint": "^8.57.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.3.3",
		"should": "^13.2.3",
		"typescript": "^5.4.5",
		"@storybook/addon-essentials": "^7.0.26",
		"@storybook/addon-interactions": "^7.0.26",
		"@storybook/addon-links": "^7.0.26",
		"@storybook/blocks": "^7.0.26",
		"@storybook/react": "^7.0.26",
		"@storybook/react-vite": "^7.0.26",
		"@storybook/testing-library": "^0.0.14-next.2",
		"@types/prettier": "2.7.3",
		"@types/react": "18.2.37",
		"@types/react-dom": "18.2.15",
		"@vitejs/plugin-react": "^4.0.2",
		"vite": "^4.4.3"
	},
	"dependencies": {
		"@anthropic-ai/bedrock-sdk": "^0.10.2",
		"@anthropic-ai/sdk": "^0.26.0",
		"@anthropic-ai/vertex-sdk": "^0.4.1",
		"@google/generative-ai": "^0.18.0",
		"@modelcontextprotocol/sdk": "^1.0.1",
		"@types/clone-deep": "^4.0.4",
		"@types/get-folder-size": "^3.0.4",
		"@types/pdf-parse": "^1.1.4",
		"@types/turndown": "^5.0.5",
		"@vscode/codicons": "^0.0.36",
		"@vscode/webview-ui-toolkit": "^1.2.2",
		"@sourcegraph/cody-shared": "file:./lib/shared",
		"@sourcegraph/cody-ui": "file:./lib/ui",
		"classnames": "^2.3.2",
		"axios": "^1.7.4",
		"cheerio": "^1.0.0",
		"chokidar": "^4.0.1",
		"clone-deep": "^4.0.1",
		"default-shell": "^2.2.0",
		"delay": "^6.0.0",
		"diff": "^5.2.0",
		"execa": "^9.5.2",
		"fast-deep-equal": "^3.1.3",
		"get-folder-size": "^5.0.0",
		"globby": "^14.0.2",
		"isbinaryfile": "^5.0.2",
		"mammoth": "^1.8.0",
		"monaco-vscode-textmate-theme-converter": "^0.1.7",
		"openai": "^4.61.0",
		"os-name": "^6.0.0",
		"p-wait-for": "^5.0.2",
		"pdf-parse": "^1.1.1",
		"puppeteer-chromium-resolver": "^23.0.0",
		"puppeteer-core": "^23.4.0",
		"serialize-error": "^11.0.3",
		"simple-git": "^3.27.0",
		"strip-ansi": "^7.1.0",
		"tree-sitter-wasms": "^0.1.11",
		"turndown": "^7.2.0",
		"web-tree-sitter": "^0.22.6",
		"zod": "^3.23.8"
	}
}